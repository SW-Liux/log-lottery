{
  "headers": [
    {
      "source": "*",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
  ],
  "redirects": [
    {
      "source": "**",
      "destination": "/index.html",
      "statusCode": 301
    },
    {
      "source": "/log-lottery/*",
      "destination": "/log-lottery/index.html",
      "statusCode": 301
    },
    {
      "source": "./",
      "destination": "/",
      "statusCode": 301
    },
  ],
  "rewrites": [
    {
      "source": "**",
      "destination": "/index.html"
    },
    {
      "source": "/log-lottery/*",
      "destination": "/log-lottery/index.html"
    },
    {
      "source": "./",
      "destination": "/"
    },
  ]
}
